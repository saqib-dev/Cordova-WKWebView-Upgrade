{
  "name": "angular2-android",
  "displayName": "HelloCordova",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
